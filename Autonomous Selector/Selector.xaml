<UserControl x:Class="Frc1360.DriverStation.Components.AutonomousSelector.Selector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Frc1360.DriverStation.Components.AutonomousSelector"
             mc:Ignorable="d" 
             Width="200" Height="150">
    <ListBox x:Name="list" Background="Transparent" BorderThickness="0">
        <ListBox.ItemTemplate>
            <DataTemplate>
                <ComboBox ItemsSource="{Binding Options}" SelectedItem="{Binding Value}" SelectedIndex="0"/>
            </DataTemplate>
        </ListBox.ItemTemplate>
    </ListBox>
</UserControl>
