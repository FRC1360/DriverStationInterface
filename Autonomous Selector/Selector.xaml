<UserControl x:Class="Frc1360.DriverStation.Components.AutonomousSelector.Selector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Frc1360.DriverStation.Components.AutonomousSelector"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" DataContext="{Binding Controller, RelativeSource={RelativeSource Self}}">
    <ListBox ItemsSource="{Binding AutonomousSelection}">
        <ListBox.ItemTemplate>
            <DataTemplate>
                <ComboBox ItemsSource="{Binding Options}" SelectedItem="{Binding Value}"/>
            </DataTemplate>
        </ListBox.ItemTemplate>
    </ListBox>
</UserControl>
